<h1>Listing models</h1>

<table>
  <tr>
    <th>Model num</th>
    <th>Type</th>
    <th>Brand</th>
    <th>Archive</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @models.each do |model| %>
  <tr>
    <td><%= model.model_num %></td>
    <td><%= model.type_id %></td>
    <td><%= model.brand_id %></td>
    <td><%= model.archive %></td>
    <td><%= link_to 'Show', model %></td>
    <td><%= link_to 'Edit', edit_model_path(model) %></td>
    <td><%= link_to 'Destroy', model, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Model', new_model_path %>
