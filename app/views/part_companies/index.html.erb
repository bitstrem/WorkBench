<% @part_companies.each do |part_company| %>
  <tr>
    <td><%= part_company.name %></td>
    <td><%= part_company.address %></td>
    <td><%= part_company.archive %></td>
    <td><%= link_to 'Show', part_company %></td>
    <td><%= link_to 'Edit', edit_part_company_path(part_company) %></td>
    <td><%= link_to 'Destroy', part_company, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

